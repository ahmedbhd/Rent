<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/det_root"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".LocDetailActivity">

    <TextView
            android:text="Cin"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="24dp"
            android:layout_marginTop="52dp"
            app:layout_constraintTop_toTopOf="parent"
            />

    <TextView
            android:text="TextView"
            android:layout_width="159dp"
            android:layout_height="24dp"
            android:id="@+id/det_text"
            app:layout_constraintStart_toEndOf="@+id/textView3"
            android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintTop_toBottomOf="@+id/det_cin"
            android:layout_marginTop="24dp"
            app:layout_constraintHorizontal_bias="0.177"/>

    <TextView
            android:text="TextView"
            android:layout_width="159dp"
            android:layout_height="24dp"
            android:id="@+id/det_cin"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="120dp"
            android:layout_marginTop="52dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView"
            android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="1.0"/>

    <TextView
            android:text="Description"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView3"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="24dp"
            android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/textView"/>

    <TextView
            android:text="Date Debut"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="24dp"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            android:layout_marginTop="32dp"/>

    <TextView
            android:text="Date Fin"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView6"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="24dp"
            app:layout_constraintTop_toBottomOf="@+id/textView5"
            android:layout_marginTop="60dp"/>

    <TextView
            android:layout_width="187dp"
            android:layout_height="36dp"
            android:ems="10"
            android:id="@+id/det_date_debu"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            android:layout_marginTop="28dp"
            app:layout_constraintTop_toBottomOf="@+id/det_text"
            app:layout_constraintHorizontal_bias="0.271"
            android:textSize="18sp"/>

    <TextView
            android:layout_width="187dp"
            android:layout_height="36dp"
            android:ems="10"
            android:id="@+id/det_date_fin"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintStart_toEndOf="@+id/textView6"
            android:layout_marginStart="8dp"
            android:layout_marginTop="44dp"
            app:layout_constraintTop_toBottomOf="@+id/det_date_debu"
            app:layout_constraintHorizontal_bias="0.371"
            android:textSize="18sp"/>

    <TextView
            android:text="Téléphone"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView7"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="24dp"
            android:layout_marginTop="68dp"
            app:layout_constraintTop_toBottomOf="@+id/textView6"/>
    <androidx.coordinatorlayout.widget.CoordinatorLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" tools:layout_editor_absoluteY="0dp"
            tools:layout_editor_absoluteX="0dp" android:id="@+id/coordinatorLayout">
        <include
                layout="@layout/bottom_sheet"
                />
    </androidx.coordinatorlayout.widget.CoordinatorLayout>


</androidx.constraintlayout.widget.ConstraintLayout>